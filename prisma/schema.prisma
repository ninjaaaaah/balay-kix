generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model profiles {
  id         String    @id @db.Uuid
  updatedat  DateTime? @db.Timestamptz(6)
  email      String?
  username   String?   @unique
  firstname  String?
  lastname   String?
  avatarlink String?
  website    String?
}
